{
    "name": "Contact Web Whatsapp",
    "description": "Contacts for Web Whatsapp",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://web.whatsapp.com/"],
            "js": ["replace.js"]
        }
    ]
  }